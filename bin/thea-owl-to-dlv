#!/usr/bin/swipl -L0 -G0 -A0 -T0 -q -g main -t halt -s

:- use_module(library('thea/owl2_util')).
:- use_module(library('thea/owl2_io')).
:- use_module(library('thea/owl2_to_prolog_dlp')).
:- use_module(library('thea/swrl_rdf_hooks')).
:- use_module(library('thea/swrl')).
:- use_module(library('semweb/rdf_turtle')).


main :-
        current_prolog_flag(argv, Arguments),
        append(_SytemArgs, [--|Args], Arguments), !,
        set_prolog_flag(verbose,normal),
        debug(owl2_bench),
        forall(member(Arg,Args),
               load_axioms(Arg,_,[])),
        use_labels_for_IRIs,
        owl_write_all_dlpterms([disjunctive_datalog(true),head_disjunction_symbol(v),no_base(_),suppress_literals(true)]).
